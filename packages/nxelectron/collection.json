{
  "name": "nxelectron",
  "version": "0.1",
  "extends": [
    "@nrwl/workspace"
  ],
  "schematics": {
    "init": {
      "factory": "./src/schematics/init/init",
      "schema": "./src/schematics/init/schema.json",
      "description": "Initialize the @bananajs/nxelectron plugin",
      "aliases": [
        "ng-add"
      ],
      "hidden": true
    },
    "application": {
      "factory": "./src/schematics/application/application",
      "schema": "./src/schematics/application/schema.json",
      "aliases": [
        "app"
      ],
      "description": "Create an electron application"
    }
  }
}
